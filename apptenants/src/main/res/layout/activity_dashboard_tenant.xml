<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DashboardTenant"
    android:background="@color/bluebg">

    <ImageView
        android:layout_width="342dp"
        android:layout_height="312dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="-60dp"
        android:src="@drawable/cornercloudlight" />

    <ImageView
        android:layout_width="90dp"
        android:layout_height="72dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="80dp"
        android:src="@drawable/logo" />

    <!--    <ImageView-->
    <!--        android:id="@+id/notif"-->
    <!--        android:layout_width="wrap_content"-->
    <!--        android:layout_height="wrap_content"-->
    <!--        android:layout_marginTop="80dp"-->
    <!--        android:layout_marginLeft="280dp"-->
    <!--        app:srcCompat="@drawable/notification" />-->

    <ImageView
        android:id="@+id/notif"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="80dp"
        android:layout_marginLeft="310dp"
        app:srcCompat="@drawable/notification"
        android:clickable="true"
        android:focusable="true"/>

    <ImageView
        android:id="@+id/helpbut"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="80dp"
        android:layout_marginLeft="340dp"
        app:srcCompat="@drawable/help" />

    <ImageView
        android:id="@+id/settings"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="80dp"
        android:layout_marginLeft="370dp"
        app:srcCompat="@drawable/settings"
        android:clickable="true"
        android:focusable="true"/>

    <ImageView
        android:id="@+id/profile"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_alignParentBottom="true"
        android:paddingBottom="20dp"
        android:layout_marginLeft="350dp"
        app:srcCompat="@drawable/profile"
        android:clickable="true"
        android:focusable="true"/>
    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/calculate_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/profile"
        android:layout_alignParentRight="true"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="10dp"
        android:backgroundTint="#A8CBD6"
        android:onClick="calculate_1"
        android:src="@drawable/ic_baseline_calculate_24" />



</RelativeLayout>